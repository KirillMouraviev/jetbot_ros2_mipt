<?xml version="1.0"?>
<robot xmlns:xacro="http://www.ros.org/wiki/xacro">

  <xacro:macro name="diffbot" params="prefix">

  
  <link name="${prefix}base_footprint"/>

  <link
    name="${prefix}base_link">
    <inertial>
      <origin
        xyz="-0.025609 0.0093176 0.034957"
        rpy="0 0 0" />
      <mass
        value="2" />
      <inertia
        ixx="0.0016616"
        ixy="-5.2805E-05"
        ixz="-3.0797E-06"
        iyy="0.0024755"
        iyz="-1.1469E-06"
        izz="0.0030448" />
    </inertial>
    <visual>
      <origin
        xyz="0 0 0"
        rpy="0 0 0" />
      <geometry>
        <mesh
          filename="package://diffdrive_jetbot/description/meshes/base_link.STL" />
      </geometry>
      <material
        name="">
        <color
          rgba="0.61176 0.65882 0.67059 1" />
      </material>
    </visual>
    <collision>
      <origin
        xyz="0 0 0"
        rpy="0 0 0" />
      <geometry>
        <mesh
          filename="package://diffdrive_jetbot/description/meshes/base_link.STL" />
      </geometry>
    </collision>
  </link>
  <!-- Fixed joint between base_footprint and base_link -->
  <joint
    name="${prefix}body_fix_joint"
    type="fixed">
    <origin
      xyz="0 0 0.035"
      rpy="-1.5708 0 0" />
    <parent
      link="${prefix}base_footprint" />
    <child
      link="${prefix}base_link" />
    <axis
      xyz="0 0 0" />
  </joint>
  <link
