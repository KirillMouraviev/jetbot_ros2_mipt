        iyz="8.53957008589798E-08"
        izz="0.000308569075147343" />
    </inertial>
    <visual>
      <!-- Shift mesh down by 0.045 along z to compensate for frame shift up -->
      <origin
        xyz="0 0 -0.045 "
        rpy="0 0 0" />
      <geometry>
        <mesh
          filename="package://diffdrive_jetbot/description/meshes/lidar_link.STL" />
